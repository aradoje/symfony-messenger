{
    "name": "bref/symfony-messenger",
    "description": "Symfony Messenger bridge to run with SQS on AWS Lambda with Bref",
    "keywords": ["bref", "symfony", "messenger", "sqs", "sns", "s3", "aws", "aws-lambda"],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Bref\\Symfony\\Messenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bref\\Symfony\\Messenger\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.127",
        "symfony/messenger": "^4.3 || ^5.0",
        "symfony/config": "^4.3 || ^5.0",
        "symfony/dependency-injection": "^4.3 || ^5.0",
        "symfony/http-kernel": "^4.3 || ^5.0",
        "symfony/yaml": "^4.3 || ^5.0",
        "bref/bref": "^0.5.13@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mnapoli/hard-mode": "^0.2.0",
        "phpstan/phpstan": "^0.12.0",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "nyholm/symfony-bundle-test": "^1.6",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1"
    }
}
